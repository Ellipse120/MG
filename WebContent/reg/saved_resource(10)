M.add("cos-pageload",function(e){var t=0,i="#pre-mask-loading",n=1e4,a={init:function(i,s){return s||(s=n),YUI.Env.windowLoaded||t>s?void this._onload(i):void(YUI.Env.windowLoaded||(e.config.win.setTimeout(function(){a.init(i,s)},100),t+=100))},_onload:function(e){this.closeMask(e||i)},closeMask:function(t){var n=e.one(t||i);n&&n.remove()}};e.namespace("mt.cos").PageLoad=a},"1.0.0",{requires:["node"]});